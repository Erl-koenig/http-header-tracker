{
  "name": "http-header-tracker",
  "version": "1.0.0",
  "description": "HTTP Header Tracker Browser Extension",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "web-ext lint",
    "start": "web-ext run",
    "build": "web-ext build --overwrite-dest",
    "package": "./package.sh"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "web-ext": "^9.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.js"
    ],
    "collectCoverageFrom": [
      "anonymization.js"
    ]
  }
}
